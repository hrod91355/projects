CREATE DATABASE users;

CREATE TABLE users (
	id INT NOT NULL,
	first_name VARCHAR(30) NOT NULL,
	last_name VARCHAR(30) NOT NULL,
	city VARCHAR(30) NOT NULL,
	age INT NOT NULL,
	PRIMARY KEY (id),
	UNIQUE (last_name)
);

INSERT INTO `users` (`id`, `first_name`, `last_name`, `city`, `age`)
VALUES
  (1, 'Joe', 'Smith', 'San Jose', 32),
  (2, 'John', 'Ramariez', 'CO Springs', 23),
  (3, 'Sarah', 'Jones', 'Denver', 35);
