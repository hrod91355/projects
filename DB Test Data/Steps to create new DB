MySql install through Homebrew
https://gist.github.com/nrollr/3f57fc15ded7dddddcc4e82fe137b58e

Tool: Sequel Pro
http://www.sequelpro.com/

CREATE DATABASE users;

CREATE TABLE users (
	user_uid INT AUTO_INCREMENT PRIMARY KEY not null,
	first_name VARCHAR(30) NOT NULL,
	last_name VARCHAR(30) NOT NULL,
	city VARCHAR(30) NOT NULL,
	age INT NOT NULL,
	user_email VARCHAR(30) NOT NULL,
	user_pwd VARCHAR(30) NOT NULL,
	PRIMARY KEY (id),
	UNIQUE (last_name)
);

INSERT INTO `users` (`user_uid`, `first_name`, `last_name`, `city`, `age`, `user_email`, `user_pwd`)
VALUES
	(1, 'Sarah', 'Jones', 'Denver', 35, 'sarahJones@email.com', '32145'),
	(2, 'John', 'Rameriez', 'CO Springs', 23, 'johnRameriez@email.com', '54321'),
	(3, 'Joe', 'Smith', 'San Jose', 32, 'joeSmith@email.com', '12345');

